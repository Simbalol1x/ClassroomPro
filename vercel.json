{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["models/**", "routes/**", "config/**", "utils/**"] }
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/server.js" }
    ]
}
